             x:Class="MECANICAPP.Views.EmpresaView.EmpresaInicioPage"
             Title="Inicio">
   
  
   
    <ContentPage.Content>
        <StackLayout BindingContext="{Binding EmpresaInicio,Mode=TwoWay}">
